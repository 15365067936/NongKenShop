body {
    font: 14px/1.5 "Helvetica Neue", Helvetica Neue, Helvetica, Arial, Hiragino Sans GB, Microsoft Yahei, sans-serif;
    -webkit-font-smoothing: antialiased
}

body, h6 {
    margin: 0
}

ul {
    margin: 0;
    padding: 0
}

li {
    list-style: none
}

 h6 {
    font-size: 100%
}

input {
    font-family: "Helvetica Neue", Helvetica Neue, Helvetica, Arial, Heiti SC, Hiragino Sans GB, Microsoft Yahei, sans-serif;
    font-size: inherit;
    line-height: inherit
}

a {
    cursor: pointer;
