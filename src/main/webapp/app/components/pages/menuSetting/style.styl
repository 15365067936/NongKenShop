.msbs-button {
    line-height: 38px;
    display: inline-block;
    font-size: 14px;
    text-align: center;
    cursor: pointer;
    color: #545a6c;
    border: 1px solid #d0d0d5;
    border-color: rgba(0, 0, 0, .15);
    border-radius: 4px;
    padding: 0 16px;
    min-width: 46px;
    background-color: #fff;
    background-repeat: no-repeat;
    background-position: center;
    overflow: visible;
    -ms-box-sizing: content-box;
    box-sizing: content-box;
    font-family: inherit
}

.msbs-button:hover {
    color: #2d3441;
    border-color: #a6a6a6;
    text-decoration: none
}

.msbs-button:active {
    background-color: #f5f5fa
}

.msbs-button-primary {
    border: 1px solid #00a5e0;
    background-color: #00a5e0;
    color: #fff
}

.msbs-button-primary:hover {
    background-color: #00b4f5;
    border-color: #00b4f5;
    color: #fff
}

.msbs-button-primary:active {
    background-color: #0092c7;
    border-color: #0092c7;
    color: #fff
}

.msbs-button-primary {
    border-color: #00a5e0
}

.msbs-text-input {
    position: relative;
    height: 34px;
    line-height: 34px;
    padding: 2px 8px;
    border: 1px solid #d0d0d5;
    border-radius: 4px;
    vertical-align: top;
    background-color: #fff;
    -webkit-transition: border-color .15s, background-color .15s;
}

input.msbs-text-input,
.msbs-text-input input {
    outline: 0;
    font-size: 100%;
    font-family: 'microsoft yahei', arial, sans-serif;
    color: #545a6c
}

input.msbs-text-input {
    height: 20px;
    line-height: 18px;
    padding: 9px 8px
}

.msbs-text-input input {
    display: block;
    width: 100%;
    line-height: 20px;
    margin: 0;
    padding: 7px 0;
    border: 0 none;
    background: 0;
    position: relative;
    z-index: 1
}

.msbs-text-input:hover {
    border-color: #ababaf
}

.msbs-text-input:focus,
,
{
    border-color: #00a5e0
}

.msbs-text-input>input:focus {
    border-color: #00a5e0
}

.msbs-text-input .msbs-text-input-placeholder {
    position: absolute;
    top: 2px;
    left: 8px;
    right: 8px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    color: #a2a9b6;
    cursor: text;
    line-height: 34px
}

.msbs-text-input-placeholder {
    left: 40px
}

.msbs-text-input-placeholder {
    left: 8px
}

.hidden {
    display: none!important
}

@-webkit-keyframes spin {
    form {
        -webkit-transform: rotate(0)
    }
    to {
        -webkit-transform: rotate(-360deg)
    }
}

@keyframes spin {
    form {
        transform: rotate(0)
    }
    to {
        transform: rotate(-360deg)
    }
}

.main-title {
    overflow: hidden;
    position: relative
}

