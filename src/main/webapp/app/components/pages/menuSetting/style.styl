.overlay-menu-preview {
    z-index: 20;
    position: fixed;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
    background-color: rgba(25, 28, 34, .88);
    text-align: center;
    overflow: auto;
    
}

.overlay-menu-preview:after {
    content: '';
    display: inline-block;
    *display: inline;
    *zoom: 1;
    vertical-align: middle;
    width: 0;
    height: 90%
}

.menu-preview {
    width: 317px;
    height: 652px;
    display: inline-block;
    position: relative;
    vertical-align: middle
}

.menu-preview .iphone-simulator {
    position: relative;
    width: 317px;
    height: 652px;
    background: url(./img/iphone-simulator.png) no-repeat left top
}

.menu-preview .title {
    position: absolute;
    top: 97px;
    left: 0;
    width: 100%;
    height: 26px;
    line-height: 26px;
    text-align: center;
    font-size: 14px;
    color: #fff
}

.menu-preview .btn-exit {
    position: absolute;
    bottom: -63px;
    left: 114px
}

.menu-preview .preview-nav {
    overflow: hidden;
    position: absolute;
    top: 200px;
    right: 20px;
    bottom: 79px;
    left: 21px
}

.menu-preview .top-nav {
    position: absolute;
    bottom: 0;
    right: 0;
    left: 0;
    height: 34px
}

.menu-preview .top-nav>ul>li {
    *z-index: 5;
    position: relative;
    float: left;
    height: 33px
}

.menu-preview .top-nav>ul>li>a {
    position: relative;
    z-index: 5;
    display: block;
    *float: left;
    *width: 100%;
    text-align: center;
    line-height: 34px;
    color: #333;
    font-size: 12px;
    background-color: #fff
}

.icon-keyboard {
    width: 34px;
    z-index: 5
}
